package anemiaDiagnosis;

unit ParametersUnit;

rule "AnemicSyndrome"
when
    $parameters : /parameters[hb < 8 && hb !=0, epo > 18.5, hematocrit < 53 , plaquets > 7.4, anysocytosis > 11.5 ,
    retyculocytes < 2.5, leucocytes > 4.5, bloodVolume == null, rbc > 4.5 && rbc < 5.3, mcv > 80 && mcv < 100,
    mchc > 310 && mchc < 360, hbUrine == false, feUrine == false, fe > 60 && fe < 170, bilirubine < 1.0 && bilirubine >= 0,
    ldh < 150 && ldh >= 0, haptoglobin > 41, ferritin > 12, b12 > 200, folicAcid > 2.7, glucocorticoids < 25 ,
    skinPalenes == true, tachycardia == true, tinnitus == true,
    palpitations == true, chestPain == true, infarction == true, dizziness == true, blurredVision == true,
    headAche == true, fatigue == true, musclePain == true, dyspnea == true, tachypnea == true, coldLimp == true,
    internHemorrage == false, externHemorrage == false, nausea == false, jaundice == false, splenomegalia == false,
    cranealBallooning == false, aplasticCrisis == false,bacterianInfection == false, osteoporosis == false,
    boneDeformation == false, maxilarBoneHyperplasia == false, brittleNails == false, tongueInflamation == false,
    diarrhoea == false, paresthesia == false, petechiae == false]
then
    // IF I WANT TO DEBUG THE CODE, I CAN PRINT HERE WHATEVER I WANT System.out.println( op );
    //System.out.println( $patient + " has Anemic Syndrome. ");
    //$parameters.setAnemia(Anemia.ANEMIC_SYNDROME);

    modify ($parameters) { setAnemia(Anemia.ANEMIC_SYNDROME)}
    System.out.println($parameters.getAnemia());

end

